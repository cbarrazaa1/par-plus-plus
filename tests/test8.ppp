program patito;
var
  int i, j, k;
  float f;

void module uno(int a, int b) {
  if (a > 0) then {
    i = a + b * j + i;
    write(i + j);
    uno(a - 1, i);
  } else {
    write(a + b);
  }
}

int module dos(int a, float g) 
var int i;
{
  i = a;
  while (a > 0) do {
    a = a - k * j;
    uno(a * 2, a + k);
    g = g * j - k;
  }

  return (i + k * j);
}

main() {
  i = 2;
  k = i + 1;
  f = 3.14;
  while (i > 0) do {
    write(dos(i + k, f * 3) + 3);
    write(i, j * 2, f * 2 + 1.5);
    i = i - k;
  }
}
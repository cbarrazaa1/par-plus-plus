program MatrixMult;
var
  int A[3][3], B[3][3], C[3][3];
  int i, j, k;

main() {
  A[0][0] = 1;
  A[0][1] = 2;
  A[0][2] = 3;
  A[1][0] = 4;
  A[1][1] = 5;
  A[1][2] = 6;
  A[2][0] = 7;
  A[2][1] = 8;
  A[2][2] = 9;
  
  B[0][0] = 9;
  B[0][1] = 8;
  B[0][2] = 7;
  B[1][0] = 6;
  B[1][1] = 5;
  B[1][2] = 4;
  B[2][0] = 3;
  B[2][1] = 2;
  B[2][2] = 1;

  for i = 0 to 2 do {
    for j = 0 to 2 do {
      for k = 0 to 2 do {
        C[i][j] = C[i][j] + A[i][k] * B[k][j];
      }
    }
  }

}
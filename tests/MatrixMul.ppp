program MatrixMul;
var
  int A[2][2];
  int B[2][2];
  int C[2][2];
  int i, j, k;

main() {
  A[0][0] = 1;
  A[0][1] = 2;
  A[1][0] = 3;
  A[1][1] = 4;

  B[0][0] = 4;
  B[0][1] = 3;
  B[1][0] = 2;
  B[1][1] = 1;

  for i = 0 to 1 do {
    for j = 0 to 1 do {
      for k = 0 to 1 do {
        C[i][j] = C[i][j] + A[i][k] * B[k][j];
      }
    }
  }

  for i = 0 to 1 do {
    for j = 0 to 1 do {
      write(C[i][j]);
    }
  }
}